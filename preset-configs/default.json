{
  "extends": "",
  "publicUrl": "/",
  "srcDir": "src",
  "staticDir": "static",
  "distDir": "dist",
  "entries": {
    "index": "src/index.js"
  },
  "pages": {
    "index": {
      "template": "static/index.html",
      "entries": "index",
      "path": ""
    }
  },
  "transforms": {
    "js": {
      "transformer": "babel",
      "config": {
        "presets": ["@babel/preset-env"],
        "plugins": [
          "@babel/plugin-transform-runtime",
          ["@babel/plugin-proposal-decorators", { "legacy": true }],
          "@babel/plugin-proposal-function-sent",
          "@babel/plugin-proposal-export-namespace-from",
          "@babel/plugin-proposal-numeric-separator",
          "@babel/plugin-proposal-throw-expressions",
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-syntax-import-meta",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-json-strings"
        ]
      }
    },
    "css": "css",
    "less": "less",
    "sass": "sass",
    "scss": "sass",
    "styl": "stylus",
    "tpl": "raw",
    "svg@js": "svg-sprite",
    "svg@css": "file",
    "svg@less": "file",
    "svg@sass": "file",
    "svg@scss": "file",
    "png": "file",
    "gif": "file",
    "jpg": "file",
    "jpeg": "file",
    "woff": "file",
    "woff2": "file",
    "eot": "file",
    "ttf": "file"
  },
  "envVariables": {},
  "targets": {
    "browsers": ["last 2 versions"]
  },
  "optimization": {
    "extractVendor": "",
    "compressImage": false
  },
  "devProxy": {},
  "deploy": {
    "target": "qiniu",
    "config": {
      "AccessKey": "",
      "SecretKey": "",
      "bucket": ""
    }
  },
  "test": {
    "setupFiles": [],
    "moduleNameMapper": {}
  },
  "engines": {
    "builder": "*"
  }
}
