{
  "extends": "",
  "publicUrl": "/",
  "srcDir": "src",
  "staticDir": "static",
  "distDir": "dist",
  "entries": {
    "index": "src/index.js"
  },
  "pages": {
    "index": {
      "template": "static/index.html",
      "entries": "index",
      "path": ""
    }
  },
  "transforms": {
    "js": {
      "transformer": "babel",
      "config": {
        "presets": ["env", "stage-2"],
        "plugins": ["babel-plugin-transform-runtime", "babel-plugin-transform-decorators-legacy"]
      }
    },
    "css": "css",
    "less": "less",
    "sass": "sass",
    "scss": "sass",
    "tpl": "raw",
    "svg@js": "svg-sprite",
    "svg@css": "file",
    "svg@less": "file",
    "svg@sass": "file",
    "svg@scss": "file",
    "png": "file",
    "gif": "file",
    "jpg": "file",
    "jpeg": "file",
    "woff": "file",
    "woff2": "file",
    "eot": "file",
    "ttf": "file"
  },
  "envVariables": {},
  "targets": {
    "browsers": ["last 2 versions"]
  },
  "optimization": {
    "extract-common": true,
    "extract-vendor": ""
  },
  "devProxy": {},
  "deploy": {
    "target": "qiniu",
    "config": {
      "AccessKey": "",
      "SecretKey": "",
      "bucket": ""
    }
  },
  "engines": {
    "builder": "*"
  }
}
