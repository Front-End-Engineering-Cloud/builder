{
  "extends": "",
  "publicUrl": "/",
  "srcDir": "src",
  "staticDir": "static",
  "distDir": "dist",
  "entries": {
    "index": "src/index.js"
  },
  "pages": {
    "index": {
      "template": "static/index.html",
      "entries": "index",
      "path": ""
    }
  },
  "transforms": {
    "js": "babel",
    "css": "css",
    "m.css": {
      "transformer": "css",
      "config": {
        "modules": true
      }
    },
    "less": {
      "transformer": "less",
      "config": {
        "options": {
          "javascriptEnabled": true
        }
      }
    },
    "m.less": {
      "transformer": "less",
      "config": {
        "modules":true,
        "options": {
          "javascriptEnabled": true
        }
      }
    },
    "tpl": "raw",
    "svg@js": "svg-sprite",
    "svg@css": "file",
    "svg@m.css": "file",
    "svg@less": "file",
    "svg@m.less": "file",
    "png": "file",
    "gif": "file",
    "jpg": "file",
    "jpeg": "file",
    "woff": "file",
    "woff2": "file",
    "eot": "file",
    "ttf": "file"
  },
  "envVariables": {},
  "targets": {
    "browsers": ["last 2 versions"]
  },
  "optimization": {
    "addPolyfill": true,
    "extractCommon": true,
    "extractVendor": "",
    "compressImage": false,
    "transformDeps": false
  },
  "devProxy": {},
  "deploy": {
    "target": "qiniu",
    "config": {
      "AccessKey": "",
      "SecretKey": "",
      "bucket": ""
    }
  },
  "test": {
    "setupFiles": [],
    "moduleNameMapper": {}
  },
  "engines": {
    "builder": "*"
  }
}
